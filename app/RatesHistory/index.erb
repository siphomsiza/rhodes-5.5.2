<div data-role="page" class="type-index" id='rates_history_index_page'>

  <div data-role="header" data-theme="b">
    <h1>Rates History for <%=@municipal_roll_register.erf_number%></h1>
    <a href="<%=url_for :controller=>:AssetInformation,:action=>:edit,:id=>@municipal_roll_register.object%>" data-direction="reverse" data-icon="back">Register</a>
    <a href="<%=Rho::RhoConfig.start_path%>" data-icon="home" data-iconpos="notext" data-direction="reverse" class="ui-btn-right jqm-home">Home</a>
  </div><!-- /header -->

  <div data-role="content" >
      <div class="content-primary">
        <ul data-role="listview">
          <li data-role="list-divider">Years<span class="ui-li-count"><%=@rates_histories.length%></span></li>
            <% @rates_histories.each do |rates_history| %>
              <li>
                <a href="<%= url_for :controller => :RatesHistory,:action=>:edit,:id=>rates_history.object%>">
                  <p><strong><%= rates_history.rate_year%> </strong></p>
                  <p><%="#{Rho::NumberHelper::number_to_currency(rates_history.rate)} / #{Rho::NumberHelper::number_to_currency(rates_history.rateable_value)}"%></p>
                </a>
              </li>
          <% end %>
         </ul>
       </div>
  </div><!-- /ui-body wrapper -->

  <div data-role="footer" data-theme="b">
     <a href="<%=url_for :controller=>:RatesHistory,:action=>:new,:query=>{:municipal_roll_register_id=>@municipal_roll_register.object}%>" data-icon="add">Add</a>
  </div><!-- /header -->

</div>